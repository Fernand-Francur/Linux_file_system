#include <stdlib.h>
#include <stdbool.h>
#include <stdint.h>
#include <stdio.h>
#include <string.h>
#include <unistd.h>
#include <sys/mman.h>
#include "fs.c"
#include "disk.h"

int main() {
  char name[10] = "Megabyteg";

  char filename_meg[16] = "Mega";

  char * read_40 = calloc(1024 * 1024, sizeof(char));
  
  mount_fs(name);

  int fd_meg = fs_open(filename_meg);
  
  
  umount_fs(name);

  free(read_40);
  return 0;
}
