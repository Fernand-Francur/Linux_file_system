#include <stdlib.h>
#include <stdbool.h>
#include <stdint.h>
#include <stdio.h>
#include <string.h>
#include <unistd.h>
#include <sys/mman.h>
#include "fs.c"
#include "disk.h"

int main() {
  char name[10] = "Megabyte";
  char filename_meg[16] = "Mega";
  char place_holder_file[16] = "Other";


  make_fs(name);
  mount_fs(name);

  fs_create(filename_meg);
  fs_create(place_holder_file);

  int fd_meg = fs_open(filename_meg);
  int fd_other = fs_open(place_holder_file);

  fs_close(fd_meg);
  fs_close(fd_other

  umount_fs(name);
  

  
  return 0;
}
